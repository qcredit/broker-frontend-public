{% extends 'base.twig' %}
{% block title %}Contact{% endblock %}
{% block container %}
<h1>Contact</h1>
<div class="row">
  <div class="col-sm-12 col-md-4">
    <p>brokers@broker.pl</p>
    <p>Lorm ipsum 28, Tallinn</p>
    <p>11550 Estonia</p>
  </div>
  <div class="col-sm-12 col-md-8">
    <form action="">
      <div class="field text name">
        <label for="">Insert your name*</label>
          <input type="text" name="name" id="name"/>
      </div>
      <div class="field text email">
        <label for="">Insert your email*</label>
          <input type="text" name="email" id="email"/>
      </div>
      <div class="field textarea email">
        <label for="">Insert your message*</label>
          <textarea name="message" id="message"></textarea>
      </div>
    </form>
  </div>
</div>
{% endblock %}
