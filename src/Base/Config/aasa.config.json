{
  "config": {
    "useApi": true,
    "apiLiveUrl": "http://crm-test.asakredyt.pl/api/v1/loans",
    "apiTestUrl": "http://crm-test.asakredyt.pl/api/v1/loans",
    "logo": "/assets/img/aasa.jpg",
    "email": "offers@aasa.global"
  },
  "requestSchema": {
    "type": "object",
    "required": [
      "loanAmount",
      "loanPeriodInMonths",
      "firstName",
      "lastName",
      "payoutMethod",
      "contactAddress",
      "mainAddress",
      "idCardNumber",
      "mobilePhoneNumber",
      "educationType",
      "housing",
      "income",
      "pin",
      "emailAddress",
      "maritalStatusType"
    ],
    "properties": {
      "income": {
        "type": "object",
        "required": [
          "sourceType",
          "netPerMonth"
        ],
        "properties": {
          "sourceType": {
            "$ref": "#/definitions/sourceTypes"
          },
          "netPerMonth": {
            "type": "number"
          }
        }
      },
      "pin": {
        "type": "string",
        "maxLength": 11,
        "pattern": "[0-9]{4}[0-3]{1}[0-9}{1}[0-9]{5}"
      },
      "contactAddress": {
        "$ref": "#/definitions/addressObject"
      },
      "mainAddress": {
        "$ref": "#/definitions/addressObject"
      },
      "account": {
        "$ref": "#/definitions/accountObject"
      },
      "idCardNumber": {
        "type": "string",
        "example": "ZZC900146"
      },
      "payoutMethod": {
        "type": "string",
        "enum": [
          "Giro",
          "Account",
          "BlueCash"
        ]
      },
      "educationType": {
        "type": "string",
        "enum": [
          "MBA",
          "MSc",
          "BA",
          "Secondary",
          "Vocational",
          "Basic",
          "Other"
        ]
      },
      "maritalStatusType": {
        "type": "string",
        "enum": [
          "Single",
          "Married",
          "MarriedDivorcing",
          "Divorced",
          "Separated",
          "Widow",
          "InformationRelationship",
          "Other"
        ]
      },
      "housing": {
        "$ref": "#/definitions/housingObject"
      },
      "loanAmount": {
        "type": "number",
        "minimum": 10,
        "example": "1000.00"
      },
      "loanPeriodInMonths": {
        "type": "number",
        "minimum": 1,
        "example": "12"
      },
      "firstName": {
        "type": "string",
        "maxLength": 50,
        "minLength": 1,
        "example": "Anders",
        "section": "personal"
      },
      "lastName": {
        "type": "string",
        "maxLength": 50,
        "minLength": 1,
        "example": "Aas"
      },
      "emailAddress": {
        "type": "string",
        "example": "test.test@aasaglobal.com",
        "pattern": "\\S+@\\S+\\.\\S+"
      },
      "mobilePhoneNumber": {
        "type": "string",
        "example": "+372987654321",
        "pattern": "^(?:\\(?\\+?48)?(?:[-\\.\\(\\)\\s]*(\\d)){9}\\)?$"
      }
    },
    "definitions": {
      "sourceTypes": {
        "type": "string",
        "enum": [
          "Employed",
          "Student",
          "NormalPension",
          "DisabilityPension",
          "Unemployed",
          "BenefitOrAlimony",
          "SelfEmployed",
          "Farmer",
          "Other"
        ]
      },
      "addressObject": {
        "required": [
          "street",
          "houseNumber",
          "postalCode",
          "city"
        ],
        "properties": {
          "street": {
            "type": "string",
            "maxLength": 100,
            "minLength": 2,
            "example": "Ul.brzozowskiego 12 m. 15"
          },
          "postalCode": {
            "type": "string",
            "example": "62-262",
            "pattern": "^[0-9]{2}-[0-9]{3}$"
          },
          "houseNumber": {
            "type": "string",
            "maxLength": 10,
            "minLength": 1,
            "example": "1"
          },
          "apartmentNumber": {
            "type": "string",
            "maxLength": 10,
            "example": "1"
          },
          "city": {
            "type": "string",
            "maxLength": 50,
            "minLength": 2,
            "example": "Zachowice"
          }
        }
      },
      "accountObject": {
        "required": [
          "accountNumber",
          "accountType",
          "accountHolder"
        ],
        "properties": {
          "accountNumber": {
            "type": "string",
            "maxLength": 26,
            "minLength": 26,
            "example": "61 1090 1014 0000 0712 1981 2874"
          },
          "accountType": {
            "type": "string",
            "enum": [
              "-",
              "Personal",
              "Joint",
              "Company"
            ]
          },
          "accountHolder": {
            "type": "string",
            "example": "Anders Aas",
            "minLength": 1
          }
        }
      },
      "housingObject": {
        "required": [
          "residentialType",
          "propertyType"
        ],
        "properties": {
          "residentialType": {
            "type": "string",
            "enum": [
              "Own",
              "Rented",
              "LivingWithFamily",
              "Other",
              "CouncilHousing",
              "HousingAssociation"
            ]
          },
          "propertyType": {
            "type": "string",
            "enum": [
              "Apartment",
              "House",
              "TerracedHouse",
              "Duplex",
              "Other"
            ]
          }
        }
      }
    }
  },
  "chooseRequestSchema": {
    "type": "object",
    "properties": {
      "paymentFreeMonth": {
        "$ref": "#/definitions/paymentFreeMonthRequest"
      },
      "signingMethod": {
        "type": "string",
        "enum": [
          "SignOnPaper",
          "CallMe",
          "SignWithBlueMedia",
          "OTP"
        ]
      },
      "signingToken": {
        "type": "string"
      },
      "generateESignUrl": {
        "type": "boolean"
      },
      "update": {
        "$ref": "#/definitions/updateRequest"
      }
    },
    "definitions": {
      "paymentFreeMonthRequest": {
        "properties": {
          "enable": {
            "type": "boolean"
          },
          "installmentDate": {
            "type": "string",
            "example": "2017-01-08"
          },
          "installmentAmount": {
            "type": "number",
            "example": "224.58"
          }
        }
      },
      "insuranceRequest": {
        "properties": {
          "enable": {
            "type": "boolean"
          },
          "aasaAsBeneficiaryConsent": {
            "type": "boolean"
          },
          "insuranceMarketingConsent": {
            "type": "boolean"
          }
        }
      },
      "updateRequest": {
        "properties": {
          "insurance": {
            "$ref": "#/definitions/insuranceRequest"
          },
          "firstPaymentDate": {
            "type": "string",
            "example": "2018-05-08"
          },
          "payoutMethod": {
            "type": "string",
            "enum": [
              "Account",
              "Pikasiirto",
              "Giro",
              "BlueCash"
            ]
          },
          "pepConsent": {
            "type": "string",
            "enum": [
              "Y",
              "N"
            ]
          },
          "pepConsentFamily": {
            "type": "string",
            "enum": [
              "Y",
              "N"
            ]
          },
          "autoGiro": {
            "type": "string",
            "default": "Y",
            "enum": [
              "Y",
              "N"
            ]
          }
        }
      }
    }
  }
}