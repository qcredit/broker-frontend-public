{
  "name": "slim/slim-skeleton",
  "description": "A Slim Framework skeleton application for rapid development",
  "keywords": ["microframework", "rest", "router", "psr7"],
  "homepage": "http://github.com/slimphp/Slim-Skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Josh Lockhart",
      "email": "info@joshlockhart.com",
      "homepage": "http://www.joshlockhart.com/"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.23",
    "slim/twig-view": "^2.3",
    "doctrine/orm": "^2.6",
    "symfony/yaml": "^4.0",
    "aasa/broker": "dev-master",
    "robmorgan/phinx": "^0.9.2",
    "respect/validation": "^1.1",
    "slim/flash": "^0.4.0",
    "google/apiclient": "^2.2",
    "bryanjhv/slim-session": "^3.5",
    "slim/csrf": "^0.8.2",
    "phpmailer/phpmailer": "^6.0",
    "justinrainbow/json-schema": "^5.2",
    "hellogerard/jobby": "^3.4",
    "syslogic/doctrine-json-functions": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "App\\": "src/",
      "Doctrine\\Tests\\": "vendor/doctrine/tests/"
    }
  },
  "config": {
    "process-timeout" : 0,
    "github-oauth": {
      "github.com": "fed68cc7e6402e95dca64fcb41324bd2a9db0d15"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/aasa-global/broker"
    }
  ],
  "scripts": {
    "start": "php -S localhost:8080 -t public index.php",
    "test": "phpunit"
  }
}
